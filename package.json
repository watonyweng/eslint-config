{
    "name": "@watonyweng/eslint-config-monorepo",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@7.9.3",
    "license": "MIT",
    "author": "Watony Weng <https://github.com/watonyweng>",
    "scripts": {
      "build": "pnpm -r build",
      "stub": "pnpm -r --parallel run stub",
      "lint": "eslint .",
      "release": "bumpp package.json packages/*/package.json --commit --push --tag"
    },
    "devDependencies": {
      "@watonyweng/eslint-config": "workspace:*",
      "@watonyweng/eslint-plugin": "workspace:*",
      "bumpp": "^8.2.1",
      "eslint": "^8.22.0",
      "typescript": "^4.7.4",
      "unbuild": "^0.8.9"
    }
  }